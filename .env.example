# Web App
NEXT_PUBLIC_APP_ENV=local
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Firebase
NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=0000000000
NEXT_PUBLIC_FIREBASE_APP_ID=1:0000000000:web:abcdef123456

# Mapbox
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token

# Analytics / Feature Flags
NEXT_PUBLIC_POSTHOG_KEY=phc_example_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
NEXT_PUBLIC_FEATURE_FLAGS_DOC=flags/global

# Optional OAuth toggles
NEXT_PUBLIC_ENABLE_OAUTH=false
NEXT_PUBLIC_APPLE_CLIENT_ID=
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
